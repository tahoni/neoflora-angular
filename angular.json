{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "neoflora": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/neoflora",
      "sourceRoot": "projects/neoflora/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/neoflora",
            "index": "projects/neoflora/src/index.html",
            "main": "projects/neoflora/src/main.ts",
            "polyfills": "projects/neoflora/src/polyfills.ts",
            "tsConfig": "projects/neoflora/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/neoflora/src/favicon.ico",
              "projects/neoflora/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "projects/neoflora/src/styles.css"
            ],
            "scripts": [
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/neoflora/src/environments/environment.ts",
                  "with": "projects/neoflora/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "neoflora:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "neoflora:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "neoflora:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/neoflora/src/test.ts",
            "polyfills": "projects/neoflora/src/polyfills.ts",
            "tsConfig": "projects/neoflora/tsconfig.spec.json",
            "karmaConfig": "projects/neoflora/karma.conf.js",
            "assets": [
              "projects/neoflora/src/favicon.ico",
              "projects/neoflora/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "projects/neoflora/src/styles.css"
            ],
            "scripts": [
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/neoflora/tsconfig.app.json",
              "projects/neoflora/tsconfig.spec.json",
              "projects/neoflora/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/neoflora/e2e/protractor.conf.js",
            "devServerTarget": "neoflora:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "neoflora:serve:production"
            }
          }
        }
      }
    },
